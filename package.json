{
    "name": "embr",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "npx nx run-many -t build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "release": "turbo build && && gradlew build && changeset version && changeset publish"
    },
    "workspaces": [
        "lib/js/*",
        "lib/jvm/*",
        "modules/*"
    ],
    "dependencies": {
        "@changesets/cli": "^2.26.2",
        "changeset": "^0.2.6",
        "npx": "^10.2.2"
    },
    "devDependencies": {
        "@nx/gradle": "^19.5.4",
        "@nx/js": "19.5.4",
        "@nx/vite": "^19.5.4",
        "@nx/web": "19.5.4",
        "@swc-node/register": "~1.9.1",
        "@swc/core": "~1.5.7",
        "@swc/helpers": "~0.5.11",
        "@vitest/coverage-v8": "^1.0.4",
        "@vitest/ui": "^1.3.1",
        "eslint": "^8.56.0",
        "nx": "19.5.4",
        "prettier": "^3.1.1",
        "turbo": "latest",
        "typescript": "~5.5.2",
        "vite": "^5.0.0",
        "vitest": "^1.3.1"
    },
    "packageManager": "npm@10.5.0"
}
